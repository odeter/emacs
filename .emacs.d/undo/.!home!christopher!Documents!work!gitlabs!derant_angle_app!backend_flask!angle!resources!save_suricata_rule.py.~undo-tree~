def read_file(rule_file):
    rules_list = []
    file = ((rule_file.read()).decode('utf-8')).splitlines()
    print(file)
    for line in file:
        print(line.rstrip())
        strip = line.rstrip()
        if not strip or strip.startswith(\"#\"):
            pass
        else:
            rules_list.append(strip)
    return rules_list

class SaveSuricataRule(Resource):
    @role_required([\"admin\", \"mssp_it_sec\", \"mssp_admin\", \"op_man\", \"it_man\", \"it_sec\", \"sys_admin\"])
    def post(self):
        parser = reqparse.RequestParser()
        parser.add_argument('files', type=werkzeug.datastructures.FileStorage, location='files', action=\"append\")
        parser.add_argument('sensorID', help = 'This field cannot be blank', location=\"form\", required=True)
        parser.add_argument('rule', location=\"form\")
        parser.add_argument('name', location=\"form\")
        parser.add_argument('description', location=\"form\")
        parser.add_argument('firmID', location=\"form\")
        data = parser.parse_args()

        userID = get_jwt_identity()
        current_user = UserModel.find_by_id(userID)
        firm_id = json.loads(data['firmID'])
        print(\"FIRMID\", data['firmID'])

        if current_user.check_role('sys_admin') and firm_id:
            firm = FirmModel.find_by_id(firm_id)
        elif current_user.check_roles([\"mssp_admin\",\"mssp_it_sec\"]) and firm_id:
            firm = current_user.firm.mssp_get_firm(firm_id)
        else:
            firm = current_user.firm

        if firm:
            files = data['files']
            # Verify that user is in firm owning the sensor
            sensor = firm.find_sensor_id(data['sensorID'])
            if sensor:
                if files:
                    rules_list = []
                    for r_file in files:
                        rules_list.extend(read_file(r_file))
                    print(rules_list)
                    pd_arr =  pd.DataFrame(rules_list, columns =['rule'])
                    if not pd_arr.empty:
                        cursor = db.engine.connect()
                        pd_arr['name'] = data['name']
                        pd_arr['description'] = data['description']
                        pd_arr['sensor_id'] = sensor.sensor_id
                        try:
                            pd_arr.to_sql('suricata_rules', cursor, method='multi', if_exists='append', index=False)
                        except:
                            app.logger.error('{0} - {1} tried importing an suricata rule file but something went wrong when saving to db'.format(current_user.name, current_user.id))
                            return {'message': 'Something went wrong'}, 500
                        return {'message': 'Suricata rule Created'}, 200
                    else:
                        return {'message': 'File contained no rules'}, 403
            elif data['rule']:
                # Reject rule name over 50 characters
                if data[\"name\"] and len(data[\"name\"]) > 50:
                    app.logger.error('{0} - {1} tried saving a suricata rule but name was too long'.format(current_user.name, current_user.id))
                    return {'message': 'Name too long', 'errtag': 'error_14'}, 400

                # Reject rule description over 200 characters
                if data[\"description\"] and len(data[\"description\"]) > 200:
                    app.logger.error('{0} - {1} tried saving a suricata rule but description was too long'.format(current_user.name, current_user.id))
                    return {'message': 'Description too long', 'errtag': 'error_14'}, 400

                # Reject rules with newlines
                if len(data[\"rule\"].splitlines()) > 1:
                    app.logger.error('{0} - {1} tried saving a suricata rule but it contained 1 or more newlines'.format(current_user.name, current_user.id))
                    return {'message': 'Rule contained newline', 'errtag': 'error_46'}, 400

                # Save suricata rule
                new_rule = SuricataRulesModel(name=data['name'],
                                              description=data['description'],
                                              rule=data['rule'].rstrip())
                try:
                    sensor.add_suricata_rule(new_rule)
                    new_rule.save_to_db()
                    app.logger.info('{0} - {1} created a new suricata rule'.format(current_user.name, current_user.id))
                    return {'message': 'Suricata rule Created'}, 200
                except:
                    app.logger.error('{0} - {1} tried creating a new suricata rule but something went wrong when saving to db'.format(current_user.name, current_user.id))
                    return {'message': 'Something went wrong'}, 500
            else:
                return {'message': 'No sensor with that ID could be found in given firm', 'errtag': 'error_45'}, 403
        else:
            return {'message': 'No firm could be found'}, 40" 0 6 (face font-lock-keyword-face) 13 16 (face font-lock-keyword-face) 17 26 (face font-lock-function-name-face) 26 27 (face (rainbow-delimiters-depth-1-face)) 36 37 (face (rainbow-delimiters-depth-1-face)) 43 53 (face font-lock-variable-name-face) 56 57 (face (rainbow-delimiters-depth-1-face)) 57 58 (face (rainbow-delimiters-depth-1-face)) 63 67 (face font-lock-builtin-face) 70 71 (face (rainbow-delimiters-depth-1-face)) 71 72 (face (rainbow-delimiters-depth-2-face)) 86 87 (face (rainbow-delimiters-depth-3-face)) 87 88 (face (rainbow-delimiters-depth-3-face)) 88 89 (face (rainbow-delimiters-depth-2-face)) 96 97 (face (rainbow-delimiters-depth-2-face)) 97 104 (face font-lock-string-face) 104 105 (face (rainbow-delimiters-depth-2-face)) 105 106 (face (rainbow-delimiters-depth-1-face)) 117 118 (face (rainbow-delimiters-depth-1-face)) 118 119 (face (rainbow-delimiters-depth-1-face)) 124 129 (face font-lock-keyword-face) 129 130 (face (rainbow-delimiters-depth-1-face)) 130 134 (face font-lock-builtin-face) 134 135 (face (rainbow-delimiters-depth-1-face)) 140 143 (face font-lock-keyword-face) 149 151 (face font-lock-keyword-face) 152 156 (face font-lock-builtin-face) 158 166 (fontified t) 166 171 (fontified t face font-lock-keyword-face) 171 172 (fontified t face (rainbow-delimiters-depth-1-face)) 172 183 (fontified t) 183 184 (fontified t face (rainbow-delimiters-depth-2-face)) 184 185 (fontified t face (rainbow-delimiters-depth-2-face)) 185 186 (fontified t face (rainbow-delimiters-depth-1-face)) 186 195 (fontified t) 195 200 (fontified t face font-lock-variable-name-face) 200 209 (fontified t) 209 214 (fontified t) 214 215 (fontified t face (rainbow-delimiters-depth-1-face)) 215 216 (fontified t face (rainbow-delimiters-depth-1-face)) 216 217 (fontified t) 217 225 (fontified t) 225 227 (fontified t face font-lock-keyword-face) 227 228 (fontified t) 228 231 (fontified t face font-lock-keyword-face) 231 238 (f
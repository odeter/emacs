# update firms;

INSERT INTO firms (zipcode, address, latitude, longitude, country, group_id) SELECT zipcode, address, latitude, longitude, country, group_id FROM users left join rolehelper on users.id=rolehelper.user_id where role_id=3;

# update sensor:

UPDATE sensor SET sensor.firm_id = users.firm_id FROM users WHERE sensor.user_id = users.firm_id;

# add new columns for connections:
ALTER TABLE conn_1300524940364263cd9a ADD COLUMN insert_ts TIMESTAMP DEFAULT NOW();

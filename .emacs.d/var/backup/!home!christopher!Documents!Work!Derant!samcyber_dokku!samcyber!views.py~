from angle import app
from flask import jsonify, url_for, render_template

@app.route('/')
def index():
    return render_template('index.html')# url_for('static', filename='index.html')
#return jsonify({'message': 'Hello, World!'})

@app.route('/<path:path>')
def catch_all(path):
    return render_template("index.html")

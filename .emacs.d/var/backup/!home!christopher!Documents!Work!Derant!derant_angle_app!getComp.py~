import requests
import pprint

auth_u_key = '159921282811016427a09e9d7305faa818daa34293b41f8e838318d874c57cc95030c1ba128d5b'
auth_c_key = '158685149494284a33dfe34d3d0469ee91cd9ec7282d2b0cac2846976a3fbc2cd4b2e8910705a02'

args = {'Authentication': auth_u_key, 'Authorization': auth_c_key}
response = (requests.get(url = 'https://www.digitaltberedskab.dk/ws/angle/company/',
                         params = args)).json()
pp = pprint.PrettyPrinter(width=80, compact=True)
pp.pprint(response)

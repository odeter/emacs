#!/bin/bash

case $1 in
   config)
        cat <<'EOM'
graph_title Insert Errors
graph_vlabel errors
load.label errors
EOM
        exit 0;;
esac

#LOGFILE="backend_proc_logs/insert.log"

LOGFILE="/home/dokku/p_storage/derant_angle/backend_proc_logs/insert.log"

DATE=$(date -d '10 hour ago' "+%Y-%m-%d %H:%M:%S")

printf "errors.value "; awk -F " - " -v date="$DATE" '$2 > date' $LOGFILE | grep -c "ERROR"
